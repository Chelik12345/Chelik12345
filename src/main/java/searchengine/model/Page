@Entity
public class Page {

    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private Long id;

    private String uri;
    private String title;
    private String content;

    // Геттеры и сеттеры
}

// SearchResult.java
public class SearchResult {

    private String uri;
    private String title;
    private String snippet;
    private double relevance;

    // Геттеры и сеттеры
}
